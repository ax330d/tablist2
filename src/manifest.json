{
    "manifest_version": 3,
    "name": "TabList",
    "version": "2.0.1",
    "description": "Replaces new tab page with a list of all open tabs and groups. Useful for managing many tabs.",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "icons": {
        "128": "assets/icons/favicon_128.png",
        "16": "assets/icons/favicon_16.png",
        "48": "assets/icons/favicon_48.png"
    },
    "options_page": "options.html",
    "permissions": [
        "tabs",
        "tabGroups",
        "storage",
        "favicon"
    ],
    "incognito": "split",
    "web_accessible_resources": [
        {
          "resources": ["_favicon/*"],
          "matches": ["<all_urls>"]
        }
    ]
}